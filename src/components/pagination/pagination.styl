.pagination
  margin-bottom 153px
  display flex
  justify-content flex-end
  >button
    &.active
      background-color #2ca7f8 
    margin 11px 3px
    font-size 12px
    padding 2px 8px
    cursor pointer
  >.jump
    display flex
    margin-right 18px
    input
      text-align center
      padding 0px
      margin 11px 6px
      background #fff
      width 38px
      height 20px
    p
      font-size 12px
  >span 
    margin 8px 5px 0px 5px
    font-size 12px